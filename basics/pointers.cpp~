#include<iostream>
#include<cmath>
using namespace std;
class Introduction{

	public:
	  static string prob1(const string s){

		    return s;
	  }

	  static void info(const string& name,int age,const string& city){

		       cout<<"name:"<<name<<endl;
			   cout<<"age:"<<age<<endl;
			   cout<<"city:"<<city<<endl;
	  }

	  static  unsigned int sum(unsigned int   num1, unsigned int num2){

		    return num1+num2;
	  }

	  static  signed int sub( unsigned int  num1, unsigned int num2){

		    return num1-num2;
	  }

	  static unsigned int mul(unsigned int num1,unsigned int num2){

		    return num1*num2;
	  }

	  static  double div(double num1,double num2){

		    if(num2==0){

				return 0;
			}

			return num1/num2;
	  }
	  static void input(){

		   double  num1;
		   double  num2;

		   cin>>num1>>num2;

		   double ans=div(num1,num2);
		   cout<<"sub:"<<ans<<endl;
		    
		}


};
int main(){

	Introduction::input();
}
